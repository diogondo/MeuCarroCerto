        <th>
            @Html.DisplayNameFor(model => model.rede_concessionaria)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.disponibilidade_de_pecas)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tempo_de_garantia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.design)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.codigo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.codigo_marca)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ano)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.codigo_tipo_carroceria)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.valor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.qtd_ocupantes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.trio_eletrico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ar_condicionado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.direcao_hidraulica)
