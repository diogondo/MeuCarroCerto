            </td>
            <td>
                @Html.DisplayFor(modelItem => item.desempenho_zero_a_cem)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.central_multimidia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.entrada_usb)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.camera_de_re)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.farol_neblina)
            </td>
