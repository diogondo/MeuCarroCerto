@model IEnumerable<MeuCarroCerto.Models.t_carros>

@{
    ViewBag.Title = "Index";
}
@{
    Layout = null;
}
<h2>Carros</h2>

<p>
    @Html.ActionLink("Adicionar carros", "Adicionar", null, new { @class = "btn btn-primary" })
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.codigo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.codigo_marca)
