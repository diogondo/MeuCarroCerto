                @Html.DisplayFor(modelItem => item.piloto_automatico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.roda_liga)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.teto_solar)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.historico_fabricante)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ano_projeto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.rede_concessionaria)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.disponibilidade_de_pecas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tempo_de_garantia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.design)
            </td>
            <td>
                @Html.ActionLink("Editar", "Editar", new { id = item.codigo })
                @Html.ActionLink("Excluir", "Excluir", new { id = item.codigo }, new { @class = "excluir" })
            </td>
        </tr>
    }

</table>
