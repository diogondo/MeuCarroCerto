        <th>
            @Html.DisplayNameFor(model => model.qtd_ocupantes)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.trio_eletrico)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ar_condicionado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.direcao_hidraulica)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sensor_de_estacionamento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.alarme)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cambio_automatico)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.desempenho_zero_a_cem)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.central_multimidia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.entrada_usb)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.camera_de_re)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.farol_neblina)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.piloto_automatico)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.roda_liga)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.teto_solar)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.historico_fabricante)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ano_projeto)
